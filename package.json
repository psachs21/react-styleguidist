    "lodash": "3.10.1",
    "marked": "0.3.5",
    "minimist": "1.2.0",
    "postcss-advanced-variables": "1.1.0",
    "postcss-loader": "0.6.0",
    "prettyjson": "1.1.3",
    "react-codemirror": "0.1.5",
    "react-docgen": "2.2.0",
